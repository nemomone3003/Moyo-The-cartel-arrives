<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- 로얄 바디슈트 -->
	<ThingDef ParentName="Moyo_ApparelBase">
		<defName>Moyo_CartelRoyalsuit</defName>
		<label>Cartel executive suit</label>
		<description>A Peculiar looking bodysuit made by the Cartel. Aimed to create an elegant wear with decent protection for the wearer. Mostly wore by important personnel of the cartel due to its cost.</description>
		<graphicData>
			<texPath>Things/Apparel/MoyoCartelroyalSuit1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1</drawSize>			
		</graphicData>
		<techLevel>Spacer</techLevel>
		<costList>
			<ComponentSpacer>1</ComponentSpacer>
			<Moyo_Depthweave>60</Moyo_Depthweave>
			<Hyperweave>35</Hyperweave>
			<Gold>5</Gold>
		</costList>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToMake>37000</WorkToMake>
			<Mass>0.5</Mass>
			<ArmorRating_Sharp>0.55</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.40</ArmorRating_Blunt>
			<ArmorRating_Heat>0.60</ArmorRating_Heat>
			<Insulation_Cold>65</Insulation_Cold>
			<Insulation_Heat>47.5</Insulation_Heat>
			<EquipDelay>3</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.2</MoveSpeed>
			<SocialImpact>0.1</SocialImpact>	
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/MoyoCartelroyalSuit</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
				<li>Middle</li>
			</layers>
			<tags>
				<li>Moyo_Executive</li>
				<li>Psychic</li>
			</tags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>MultiAnalyzer</researchPrerequisite>
			<skillRequirements>
				<Crafting>8</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>TableMachining</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedHealthItemBionic</unfinishedThingDef>			
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>100</weight>
					<only>(1,1,1)</only>
				</li>
			</options>
		</colorGenerator>	
	</ThingDef>


	<!-- 초인병 슈트 -->
	<ThingDef ParentName="Moyo_PowerArmorBase">
		<defName>Moyo_CEsuit</defName>
		<label>Cartel executioner suit</label>
		<description>An advanced suit of armor specifically designed for the most elite operative of the cartel.It has thinner armor than conventional powered armor, but it compensates its flaw with greatly improved mobility and built-in damage control unit.</description>
		<graphicData>
			<texPath>Things/Apparel/MoyoCEsuit1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1</drawSize>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MarketValue>3350</MarketValue>
			<MaxHitPoints>600</MaxHitPoints>
			<Mass>12</Mass>
			<Flammability>0.2</Flammability>
			<ArmorRating_Sharp>0.99</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
			<ArmorRating_Heat>0.6</ArmorRating_Heat>
			<EquipDelay>11</EquipDelay>
			<Insulation_Cold>30</Insulation_Cold>
			<Insulation_Heat>30</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>1.2</MoveSpeed>
			<MeleeDodgeChance>0.5</MeleeDodgeChance>
			<IncomingDamageFactor>-0.3</IncomingDamageFactor>
		</equippedStatOffsets>
		<apparel>
			<wornGraphicPath>Things/Apparel/MoyoCEsuit</wornGraphicPath>
			<layers Inherit="False">
				<li>OnSkin</li>
				<li>Middle</li>
			</layers>
			<tags>
				<li>Moyo_AssasinElite</li>
				<li>PrestigeCombatGear</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>100</weight>
					<only>(1,1,1)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>
	<!-- 초인병 헬멧 -->
	<ThingDef ParentName="Moyo_PowerHelmetBase">
		<defName>Moyo_CEhelmet</defName>

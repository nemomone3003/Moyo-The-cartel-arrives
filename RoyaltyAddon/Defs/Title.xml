<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<TraderKindDef>
		<defName>MoyoCartel_Caravan_TributeCollector</defName>
		<label>royal tribute collector</label>
		<requestable>True</requestable>
		<commonality>1</commonality>
		<category>TributeCollector</category>
		<tradeCurrency>Favor</tradeCurrency>
		<faction>Moyo_Cartel</faction>
		<hideThingsNotWillingToTrade>True</hideThingsNotWillingToTrade>
		<stockGenerators>
			<!-- Buying -->
			<li Class="StockGenerator_BuySlaves" />
			<li Class="StockGenerator_BuySingleDef">
				<thingDef>Gold</thingDef>
			</li>
			<li Class="StockGenerator_BuySingleDef">
				<thingDef>Moyo_AbyssPearl</thingDef>
			</li>
		</stockGenerators>
	</TraderKindDef>

	<RoyalTitleDef Abstract="True" Name="MoyoCartelTitleBase">
		<tags>
			<li>MoyoCartelTitle</li>
		</tags>
		<awardThought>GainedTitle</awardThought>
		<lostThought>LostTitle</lostThought>
		<allowDignifiedMeditationFocus>False</allowDignifiedMeditationFocus>
		<awardWorkerClass>RoyalTitleAwardWorker_Instant</awardWorkerClass>
	</RoyalTitleDef>

	<!-- 기본 맴버십 -->
	<RoyalTitleDef ParentName="MoyoCartelTitleBase">
		<defName>Cartel_Newcomer</defName>
		<label>person of interest</label>
		<description>You have made yourself known through interactions with the cartel. You have been qualified as a customer of the cartel and they are looking forward to strenghten this relationship.</description>
		<seniority>100</seniority>
		<favorCost>5</favorCost>
		<recruitmentResistanceOffset>10</recruitmentResistanceOffset>
		<commonality>0</commonality>
		<canBeInherited>False</canBeInherited>
		<permitPointsAwarded>1</permitPointsAwarded>
		<changeHeirQuestPoints>1000</changeHeirQuestPoints>
	</RoyalTitleDef>
	<!-- 중급 맴버십 -->
	<RoyalTitleDef ParentName="MoyoCartelTitleBase">
		<defName>Cartel_AlphaMember</defName>
		<label>Alpha-collaborator</label>
		<description>Qualified as a collaborator of the cartel, They have granted you an Alpha membership. also, Limited access to cartel service is now possible.</description>
		<seniority>200</seniority>
		<favorCost>8</favorCost>
		<minExpectation>Low</minExpectation>
		<recruitmentResistanceOffset>15</recruitmentResistanceOffset>
		<commonality>0</commonality>
		<canBeInherited>False</canBeInherited>
		<permitPointsAwarded>3</permitPointsAwarded>
		<changeHeirQuestPoints>1500</changeHeirQuestPoints>
	</RoyalTitleDef>
	<!-- 비즈니스 파트너 -->
	<RoyalTitleDef ParentName="MoyoCartelTitleBase">
		<defName>Cartel_BetaMember</defName>
		<label>Beta-collaborator</label>
		<description>As a Beta-collaborator, your place in the cartel is firmly set. the cartel appriciates your doing and more goods and services will be provided to you.</description>
		<seniority>300</seniority>
		<favorCost>13</favorCost>
		<minExpectation>Moderate</minExpectation>
		<recruitmentResistanceOffset>25</recruitmentResistanceOffset>
		<commonality>0</commonality>
		<canBeInherited>False</canBeInherited>
		<permitPointsAwarded>5</permitPointsAwarded>
		<changeHeirQuestPoints>3000</changeHeirQuestPoints>
		<requiredApparel>
			<li>
				<bodyPartGroupsMatchAny>
					<li>Torso</li>
				</bodyPartGroupsMatchAny>
				<requiredTags>
					<li>RoyalTier2</li>
				</requiredTags>
				<allowedTags>
					<li>Cape</li>
					<li>PrestigeCombatGear</li>
					<li>Psychic</li>
				</allowedTags>
			</li>
			<li>
				<bodyPartGroupsMatchAny>
					<li>UpperHead</li>
					<li>FullHead</li>
				</bodyPartGroupsMatchAny>
				<requiredTags>
					<li>RoyalTier2</li>
				</requiredTags>
				<allowedTags>
					<li>PrestigeCombatGear</li>
					<li>Psychic</li>
				</allowedTags>
			</li>
		</requiredApparel>
		<bedroomRequirements>
			<li Class="RoomRequirement_Area">
				<area>35</area>
			</li>
			<li Class="RoomRequirement_TerrainWithTags">
				<labelKey>RoomRequirementAllFloored</labelKey>
				<tags>
					<li>Floor</li>
					<li>FineFloor</li>
				</tags>
			</li>
			<li Class="RoomRequirement_ThingAnyOf">
				<things>
					<li>DoubleBed</li>
					<li>RoyalBed</li>
				</things>
			</li>
			<li Class="RoomRequirement_ForbiddenBuildings">
				<labelKey>RoomRequirementNoProductionFacilities</labelKey>
				<buildingTags>
					<li>Production</li>
				</buildingTags>
			</li>
		</bedroomRequirements>
	</RoyalTitleDef>
